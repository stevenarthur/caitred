<%= f.simple_fields_for f.object.address do |builder| %>
  <%= builder.input :company %>
  <%= builder.input :line_1 %>
  <%= builder.input :line_2 %>
  <div class="row">
    <div class="col-xs-6">
      <%= builder.input :suburb %>
    </div>
    <div class="col-xs-6">
      <%= builder.input :postcode %>
    </div>
  </div>
  <%= builder.input :parking_information, as: :text %>
<% end %>
